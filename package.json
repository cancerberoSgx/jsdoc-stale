{
  "name": "jsdoc-stale",
  "version": "0.0.0",
  "description": "JSDoc 3 Template / Theme inspired by slate",
  "main": "publish.js",
  "scripts": {
    "start": "node-sass --watch  --recursive scss --output static/styles/",
    "build": "node-sass --update --recursive scss --output static/styles/",
    "install": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taye/jsdoc-stale.git"
  },
